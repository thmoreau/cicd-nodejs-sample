{
	"extends": "eslint:recommended",
	"rules": {
		"no-console": "warn",
		"no-empty": "warn",
		"no-extra-boolean-cast": "warn",
		"no-mixed-spaces-and-tabs": "warn",
		"no-useless-escape": "warn",
		"no-unused-vars": "warn",
		"semi": "warn"
	},
	"env": {
		"amd": true,
		"browser": true,
		"mocha": true,
		"node": true,
    "es6": true
	}
}